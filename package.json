{
  "name": "solutionjs",
  "version": "1.0.1",
  "description": "A NPM Package for some cases solutions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Me5uT/SolutionJs.git"
  },
  "keywords": [],
  "author": "Mesut Çağa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Me5uT/SolutionJs/issues"
  },
  "homepage": "https://github.com/Me5uT/SolutionJs#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2"
  }
}
